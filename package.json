{
  "name": "webpack-sass-example",
  "description": "webpack sass/scss setting example",
  "version": "1.4.0",
  "author": "Joseph7451797",
  "repository": {
    "type": "git",
    "url": "https://github.com/Joseph7451797/webpack-sass-example.git"
  },
  "license": "MIT",
  "scripts": {
    "hot-dev": "NODE_ENV=hot-dev ./node_modules/.bin/webpack-dev-server --config webpack.config.js --hot --progress --colors --port 8080 --inline",
    "host": "NODE_ENV=host ./node_modules/.bin/webpack-dev-server --config webpack.config.js --no-info --hot --inline --host $(ifconfig | grep -Eo 'inet (addr:)?([0-9]*\\.){3}[0-9]*' | grep -Eo '([0-9]*\\.){3}[0-9]*' | grep -v '127.0.0.1')",
    "build": "NODE_ENV=build ./node_modules/.bin/webpack --config webpack.config.js --progress --profile --colors"
  },
  "devDependencies": {
    "compass-mixins": "^0.12.10",
    "css-loader": "^0.27.1",
    "file-loader": "^0.10.1",
    "node-sass": "^3.13.1",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.12.0"
  },
  "keywords": [
    "webpack",
    "sass"
  ]
}
